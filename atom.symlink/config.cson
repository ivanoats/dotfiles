  "git-plus": {}
  "haskell-ghc-mod": {}
  "ide-flow":
    flowPath: "/usr/local/bin/flow"
  "ide-purescript": {}
  "language-babel": {}
  "language-elm":
    elmOraclePath: "/Users/ivan/.nvm/versions/node/v6.7.0/bin/elm-oracle"
  linter:
    statusBar: "Show error of the selected line"
  "linter-elm-make":
    elmMakeExecutablePath: "/usr/local/bin/elm-make"
  "linter-eslint":
    disableWhenNoEslintrcFileInPath: false
    globalNodePath: "/Users/ivan/.nvm/versions/node/v6.7.0"
    showRuleIdInMessage: true
  "linter-jsonlint": {}
  "linter-stylelint":
    disableWhenNoConfig: false
    useStandard: true
  "linter-tidy":
    tidyExecutablePath: "/usr/local/bin/tidy"
  minimap:
    autoToggle: false
    plugins:
      "minimap-autohide": true
      "minimap-autohideDecorationsZIndex": 0
  nitrogen:
    showWelcomeOnStartup: false
  "one-dark-ui": {}
  react: {}
  "toggle-quotes": {}
  "tree-view":
