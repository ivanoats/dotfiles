"*":
  "atom-ternjs": {}
  "atom-typescript": {}
  "autocomplete-haskell": {}
  "autocomplete-plus": {}
  build:
    buildOnSave: true
    panelVisibility: "Show on Error"
    saveOnBuild: true
  core:
