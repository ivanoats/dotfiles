      "term2"
      "language-swig"
      "grunt-runner"
      "nitrogen"
      "build"
      "css-color-underline"
      "react"
      "parcel"
    ]
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      ".unison"
      "CVS"
      ".AppleDouble"
      ".LSOverride"
      ".Spotlight-V100"
      ".Trashes"
      "desktop.ini"
      "*.lock"
      "*.sock"
      "*.pid"
      "*.sw?"
      ".unison*"
      "*~"
    ]
    projectHome: "/Users/ivan/dev"
    telemetryConsent: "limited"
    themes: [
      "unity-ui"
      "solarized-light-syntax"
    ]
  deprecated:
    tool:
      grunt: true
  editor:
    fontFamily: "Fira Code"
    invisibles: {}
